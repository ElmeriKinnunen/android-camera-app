{
  "expo": {
    "name": "android-camera-app",
    "slug": "android-camera-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "scss", "sass"]
    },

    "android": {
      "package": "com.main.android",
      "versionCode": 1,
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "CAMERA_ROLL"
      ],
      "config": {
        "branch": {
          "apiKey": "xxx-xxxx-xxx"
        },
        "fabric": {
          "apiKey": "xxx-xxxx-xxx"
        }
      },
      "splash": {
        "backgroundColor": "#ffffff",
        "resizeMode": "contain"
      }
    }
  }
}
